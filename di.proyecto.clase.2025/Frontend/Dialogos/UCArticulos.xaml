<UserControl
    x:Class="di.proyecto.clase._2025.Frontend.Dialogos.UCArticulos"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:di.proyecto.clase._2025.Frontend.Dialogos"
    xmlns:mat="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="80"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <DockPanel>
        <Grid DockPanel.Dock="Top">
            <ToolBar>
                <Button
                    x:Name="btnAgregarModelo"
                    Click="btnAgregarModelo_Click"
                    ToolTip="Agregar ModeloArticulo">
                    <mat:PackIcon Kind="PlusBoxMultipleOutline" />

                </Button>
                <Button
                    x:Name="btnAgregarArticulo"
                    Click="btnAgregarArticulo_Click"
                    ToolTip="Agregar Articulo">
                    <mat:PackIcon Kind="PlusBoxMultipleOutline" />

                </Button>
            </ToolBar>
        </Grid>
    </DockPanel>
</UserControl>
